{
  "parser": "babel-eslint",
  "extends": ["airbnb/base"],
  "env": {
    "browser": true,
    "jasmine": true
  },
  "globals": {
    "$": true,
    "$j": true,
    "angular": true,
    "inject": true,
    "module": true
  },
  "rules": {
    "max-len": 0,
    "no-param-reassign": 0,
    "no-underscore-dangle": 0,
    "class-methods-use-this": 0,
    "import/no-extraneous-dependencies": ["error", {"devDependencies": ["**/*.spec.js"]}],
  }
}
